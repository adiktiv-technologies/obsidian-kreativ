{
	"name": "obsidian-kreativ",
	"version": "0.0.1",
	"description": "Dynamically interact with Obsidian notes using local LLMs for content summarization, idea generation, and note connections.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs && node copy-modules.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production && node copy-modules.mjs",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"rebuild": "electron-rebuild -v 30.1.0 -f -w onnxruntime-node"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/adiktiv-technologies/obsidian-kreativ"
	},
	"keywords": [
		"ai",
		"obsidian",
		"plugin",
		"LocalAI",
		"llm",
		"transformers"
	],
	"author": "Adiktiv Technologies, Inc.",
	"homepage": "https://github.com/adiktiv-technologies/obsidian-kreativ",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^16.18.126",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"electron-rebuild": "^3.2.9",
		"esbuild": "0.20.0",
		"esbuild-plugin-copy": "^2.1.1",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"@huggingface/transformers": "^3.7.6",
		"onnxruntime-node": "^1.14.0",
		"onnxruntime-web": "^1.22.0-dev.20250409-89f8206ba4"
	}
}
